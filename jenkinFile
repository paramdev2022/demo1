pipeline{
    agent any
    stages{
              
        stage(build){
            steps{
               sh 'mvn clean install -DskipTests'
               echo "cleaning 1 2 3 4 5 6"
            }
        }
        stage(test){
            steps{
               sh 'mvn test'
            }
        post{
            always{
                junit "target/surefire-reports/*.xml"
                archiveArtifacts 'target/*.jar'
            }    
            }
        }
        
    }
}
